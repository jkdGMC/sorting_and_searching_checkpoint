ALGORITHM inser_sort
VAR
  i,j,n,temp : INTEGER;
  tab : ARRAY_OF INTEGER[5];
BEGIN

  n := tab.length
  FOR i FROM 0 TO n - 1 STEP 1 DO
    Read(tab[i])
  END_FOR

  FOR i FROM 1 TO n - 1 STEP 1 DO
  
    temp := tab[i];
    j := i -1;

    WHILE (j >= 0 AND temp < tab[j]) DO
      tab[j+1] := tab[j];
      j := j - 1;
    END_WHILE

  END_FOR
  
END
// [4,2,3,1]
